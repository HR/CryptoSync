{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "semi": ["error", "never"],
    "comma-style": ["error", "last"]
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "html"
  ]
}
